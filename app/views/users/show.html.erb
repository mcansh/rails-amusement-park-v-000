<% if @message %>
  <div class="message">
    <p><%= @message %></p>
  </div>
<% end %>

<% if @user.admin %>
  <p>ADMIN</p>
<% end %>

<p>Name: <%= @user.name %></p>
<p>Mood: <%= @user.mood %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Height: <%= @user.height %></p>

<%= link_to "See attractions", attractions_path %>
